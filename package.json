		"topgroup",
		"dx",
		"digitop",
		"dig-cli",
		"deploy k8s",
		"diginext-cli",
		"diginext",
		"di",
		"di-cli",
		"build-server",
		"cli-deploy",
		"k8s",
		"kubernetes",
		"kubectl",
		"build container",
		"build docker",
		"deploy docker",
		"run k8s"
